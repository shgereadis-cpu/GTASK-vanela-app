modules = ["web", "python-3.11", "nodejs-20"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Node.js Server"

[[workflows.workflow]]
name = "Node.js Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server.js"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 40757
externalPort = 3001

[[ports]]
localPort = 46131
externalPort = 3002

[[ports]]
localPort = 46737
externalPort = 3000

[deployment]
deploymentTarget = "static"
publicDir = "."
